SELECT t2.title, t6.start_date, t6.end_date
  FROM
      hsdb45_dent_admin.link_course_student AS t1
    INNER JOIN
      hsdb45_dent_admin.course AS t2
      ON t2.course_id = t1.parent_course_id
    INNER JOIN
      tusk.academic_level_course AS t3
      ON (t3.course_id = t1.parent_course_id AND t2.course_id = t3.course_id)
    INNER JOIN 
      tusk.academic_level AS t4
      ON t3.academic_level_id = t4.academic_level_id
    INNER JOIN
      tusk.academic_level_clinical_schedule AS t5
      ON (t4.academic_level_id = t5.academic_level_id AND t3.academic_level_id = t5.academic_level_id)
    INNER JOIN
      hsdb45_dent_admin.time_period AS t6
      ON t6.time_period_id = t1.time_period_id
    WHERE (t4.school_id = '1' AND t1.child_user_id = 'tus3185');