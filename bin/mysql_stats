use FindBin;
use lib "$FindBin::Bin/../lib";

use strict;
use MySQL::Password;
my ($un,$pw) = get_user_pw;

system("mysql -p$pw -u $un -h 127.0.0.1 -P 3308 $ARGV[0]");

